<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="PropDev-InitiatePage" parent="PropDev-InitiatePage-parentBean"/>
    <bean id="PropDev-InitiatePage-parentBean" parent="Uif-DocumentPage">
        <property name="items">
            <list merge="false">
                <bean parent="PropDev-InitiateSection"/>
            </list>
        </property>
        <property name="header.headerText" value="Create Proposal"/>
        <property name="header.lowerGroup">
            <bean parent="Uif-VerticalBoxGroup">
                <property name="items">
                    <list>
                        <bean parent="Uif-Message" p:messageText="[p]* indicates required fields[/p]"
                              p:cssClasses="text-muted"/>
                    </list>
                </property>
            </bean>
        </property>
        <property name="footer">
					<null/>
        </property>
    </bean>

    <bean id="PropDev-InitiateSection" parent="PropDev-InitiateSection-parentBean"/>
    <bean id="PropDev-InitiateSection-parentBean" abstract="true" parent="PropDev-DefaultDetails-Section"/>
</beans>